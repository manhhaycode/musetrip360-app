{
  "name": "musetrip360-app",
  "private": true,
  "version": "0.0.1",
  "description": "MuseTrip360 - A comprehensive platform for museums and exhibitions",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "preinstall": "npx only-allow pnpm",
    "check-deps": "manypkg check",
    "fix-deps": "manypkg fix",
    "clean": "turbo run clean && rm -rf node_modules",
    "test": "turbo run test",
    "prepare": "husky"
  },
  "devDependencies": {
    "@manypkg/cli": "^0.24.0",
    "@types/node": "^20",
    "eslint": "^9",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.0",
    "tsup": "^8.0.1",
    "turbo": "^2.5.2",
    "typescript": "^5"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manhhaycode/musetrip360-app"
  }
}
