{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"],
      "outputs": []
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "outputs": ["dist/**"],
      "cache": false,
      "persistent": true
    },
    "start": {
      "persistent": true,
      "cache": false
    }
  },
  "ui": "tui"
}
