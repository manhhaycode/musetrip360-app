{
  "name": "@musetrip360/museum-management",
  "version": "0.1.0",
  "description": "A comprehensive museum management system package for the MuseTrip360 system with domain logic, API integration, state management, and UI components",
  "type": "module",
  "main": "./dist/es/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./api": {
      "types": "./dist/types/api.d.ts",
      "import": "./dist/es/api/index.js"
    },
    "./state": {
      "types": "./dist/types/state.d.ts",
      "import": "./dist/es/state/index.js"
    },
    "./ui": {
      "types": "./dist/types/ui.d.ts",
      "import": "./dist/es/ui/index.js"
    },
    "./domain": {
      "types": "./dist/types/domain.d.ts",
      "import": "./dist/es/domain/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly --declaration --outDir dist",
    "build:watch": "vite build --watch",
    "dev": "vite build --watch --emptyOutDir=false && tsc --emitDeclarationOnly --declaration --outDir dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@musetrip360/infras": "workspace:*",
    "@musetrip360/query-foundation": "workspace:*",
    "@musetrip360/ui-core": "workspace:*",
    "@musetrip360/user-management": "workspace:*",
    "@tailwindcss/vite": "^4.1.8",
    "@tanstack/react-query": "^5.81.5",
    "immer": "^10.1.1",
    "react-hook-form": "^7.58.1",
    "tailwindcss": "^4",
    "zod": "^3.25.67",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@musetrip360/eslint-config": "workspace:*",
    "@musetrip360/tsconfig": "workspace:*",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "eslint": "^9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-scrollbar": "^4.0.2",
    "typescript": "^5",
    "typescript-eslint": "^8.32.0",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^3.7.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.4"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/musetrip360-app.git",
    "directory": "packages/auth-system"
  },
  "keywords": [
    "musetrip360",
    "museum",
    "authentication",
    "auth",
    "oauth",
    "react",
    "typescript",
    "api",
    "state-management",
    "ui-components"
  ],
  "author": "MuseTrip360 Team",
  "license": "MIT"
}
