{
  "name": "@musetrip360/query-foundation",
  "version": "0.1.0",
  "description": "React Query foundation with API client setup, caching strategies, and offline functionality for MuseTrip360 system",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch",
    "dev": "vite build --watch --emptyOutDir=false",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "peerDependencies": {
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@musetrip360/eslint-config": "workspace:*",
    "@musetrip360/tsconfig": "workspace:*",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "eslint": "^9",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "typescript": "^5",
    "typescript-eslint": "^8.32.0",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "@musetrip360/infras": "workspace:*",
    "@tanstack/query-async-storage-persister": "^5.81.5",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-query-persist-client": "^5.81.5",
    "axios": "^1.10.0",
    "idb": "^8.0.0",
    "zustand": "^5.0.6"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/musetrip360-app.git",
    "directory": "packages/query-foundation"
  },
  "keywords": [
    "musetrip360",
    "museum",
    "react",
    "typescript",
    "tanstack-query",
    "react-query",
    "api-client",
    "offline-first"
  ],
  "author": "MuseTrip360 Team",
  "license": "MIT"
}
