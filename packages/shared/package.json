{
  "name": "@musetrip360/shared",
  "version": "0.1.0",
  "description": "A comprehensive shared package for the MuseTrip360 system",
  "type": "module",
  "main": "./dist/es/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./api": {
      "types": "./dist/types/api.d.ts",
      "import": "./dist/es/api/index.js"
    },
    "./types": {
      "types": "./dist/types/types.d.ts",
      "import": "./dist/es/types/index.js"
    },
    "./ui": {
      "types": "./dist/types/ui.d.ts",
      "import": "./dist/es/ui/index.js"
    },
    "./utils": {
      "types": "./dist/types/utils.d.ts",
      "import": "./dist/es/utils/index.js"
    },
    "./hooks": {
      "types": "./dist/types/hooks.d.ts",
      "import": "./dist/es/hooks/index.js"
    },
    "./contexts": {
      "types": "./dist/types/contexts.d.ts",
      "import": "./dist/es/contexts/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch",
    "dev": "vite build --watch --emptyOutDir=false",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "peerDependencies": {
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@musetrip360/auth-system": "workspace:*",
    "@musetrip360/infras": "workspace:*",
    "@musetrip360/query-foundation": "workspace:*",
    "@musetrip360/ui-core": "workspace:*",
    "@react-spring/web": "^10.0.1",
    "@tailwindcss/vite": "^4.1.8",
    "@tanstack/react-query": "^5.81.5",
    "file-saver": "^2.0.5",
    "js-file-download": "^0.4.12",
    "lucide-react": "^0.525.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.58.1",
    "tailwindcss": "^4",
    "zod": "^3.25.67",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@musetrip360/eslint-config": "workspace:*",
    "@musetrip360/tsconfig": "workspace:*",
    "@types/file-saver": "^2.0.7",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "eslint": "^9",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "typescript": "^5",
    "typescript-eslint": "^8.32.0",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^3.1.4"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/musetrip360-app.git",
    "directory": "packages/shared"
  },
  "keywords": [
    "musetrip360",
    "museum",
    "user-management",
    "react",
    "typescript",
    "api",
    "state-management",
    "ui-components"
  ],
  "author": "MuseTrip360 Team",
  "license": "MIT"
}
